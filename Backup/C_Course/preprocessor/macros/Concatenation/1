/*

This program explains how to concatenate two tokens

Author	: Vivek Tiwari
Usage	: #define MACRO_NAME MACRO_BODY
VERSION	: 1.0

*/

#include<stdio.h>
#include<string.h>

	#define NUMBER(s) 	 s ## e3// Converting Macro argument into string constant

int main()
{
	float a = NUMBER(10);
	printf("\n Name of Organizations is %.2e\n",a) ;
	
}
